{
    "functions": [
      {
        "name": "freetutor-question-gateway",
        "runtime": "nodejs14.x",
        "handler": "index.handler",
        "events": [
          {
            "http": {
              "path": "/create",
              "method": "POST"
            }
          },
          {
            "http": {
              "path": "/getquestion",
              "method": "GET"
            }
          },
          {
            "http": {
              "path": "/health",
              "method": "GET"
            }
          }
        ]
      }
    ],
    "exclude": [
      "config.js",
      "home.js"
    ]
  }